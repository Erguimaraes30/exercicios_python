a = 2
b = 12
c = -14

delta = (b ** 2) - (4 * a * c)

Raiz_1 = (-b + (delta) ** 0.5) / (2 * a)
Raiz_2 = (-b - (delta) ** 0.5) / (2 * a)

print("Raiz 1:", Raiz_1)
print("Raiz 2:", Raiz_2)

